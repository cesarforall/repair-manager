<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ClientesListController">
   <children>
      <TableView fx:id="tablaClientes" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <columns>
            <TableColumn fx:id="columnaId" prefWidth="100" text="Id" />
            <TableColumn fx:id="columnaNombre" prefWidth="150" text="Nombre" />
            <TableColumn fx:id="columnaDetalle" prefWidth="200" text="Detalle" />            
         </columns>
      </TableView>
   </children>
</AnchorPane>
