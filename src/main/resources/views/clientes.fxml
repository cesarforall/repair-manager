   <children>
      <TableView fx:id="tablaClientes" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="columnaId" text="Id" prefWidth="150" />
            <TableColumn fx:id="columnaNombre" text="Nombre" prefWidth="150" />
            <TableColumn fx:id="columnaDetalle" text="Detalle" prefWidth="200" />
         </columns>
      </TableView>
   </children>
</AnchorPane>